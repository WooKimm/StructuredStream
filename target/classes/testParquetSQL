create env spark(
    spark.default.parallelism='2',
    spark.sql.shuffle.partitions='2'
)WITH(
    appname='ParquetTest'
);

CREATE TABLE parquetTable(
    name string,
    age int64
)WITH(
    type='parquet',
    processwindow='10 seconds,10 seconds',
    path='filepath/parquet/people.parquet'
);

create SINK console(
)WITH(
    type='console',
    outputmode='update',
);

insert into console select * from parquetTable;